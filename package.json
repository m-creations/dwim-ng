{
  "name": "dwim_ng",
  "version": "0.0.1",
  "main": "lib/dwim_ng.js",
  "description": "A library containing Angular and PrimeNG to be used in dwim.hu Common Lisp application server",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m-creations/dwim-ng.git"
  },
  "keywords": [
    "dwim.hu",
    "common lisp",
    "angular",
    "primeng",
    "webpack",
    "library",
    "umd"
  ],
  "author": "Kambiz Darabi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-creations/dwim-ng/issues"
  },
  "homepage": "https://github.com/m-creations/dwim-ng",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "@angular/animations": "^5.2.10",
    "@angular/common": "^5.2.10",
    "@angular/compiler": "^5.2.10",
    "@angular/core": "^5.2.10",
    "@angular/forms": "^5.2.10",
    "@angular/platform-browser": "^5.2.10",
    "@angular/platform-browser-dynamic": "^5.2.10",
    "@angular/router": "^5.2.10",
    "font-awesome": "^4.7.0",
    "primeng": "^5.2.6",
    "rxjs": "^5.2.2",
    "zone.js": "^0.8.14"
  }
}
