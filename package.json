{
  "name": "hu.dwim.primeng",
  "version": "0.0.1",
  "description": "A library containing PrimeNG to be used in dwim.hu Common Lisp application server",
  "main": "lib/hu.dwim.primeng.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/m-creations/hu.dwim.primeng.git"
  },
  "keywords": [
    "dwim.hu",
    "common lisp",
    "angular",
    "primeng",
    "webpack",
    "library",
    "umd"
  ],
  "author": "Kambiz Darabi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/m-creations/hu.dwim.primeng/issues"
  },
  "homepage": "https://github.com/m-creations/hu.dwim.primeng",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  }
}
